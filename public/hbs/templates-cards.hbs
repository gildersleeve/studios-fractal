<!-- SECTION -->
<div class="section main {{class}}" data-section="{{newSection}}">
  <div class="mainBG u-background-size-cover" style="background-image: url('');" data-src="{{{ get 'imageBackground' }}}"></div>
  <div class="hero">
    <div class="heroInner u-background-size-cover" style="background-image: url('');" data-src="{{{ get 'imageHero' }}}">
      <!-- HERO ELEMENTS HERE (POPOVERS, TILES, CAROUSEL, ETC)-->

      <!--X HERO ELEMENTS HERE -->
    </div>
  </div>

  <div class="content">
    <div class="contentBG"></div>
    <div class="contentInner">
      <div class="contentInnerBG"></div>
      <div class="contentHeader">

        {{{ get 'contentHeading' }}}

      </div>
      <div class="contentCopy">

        {{{ get 'contentText' }}}

      </div>
      <!-- INLINE ELEMENTS HERE (CARDS, TILES, ACCORDIANS, ETC-->

      <div class="flexible">

        {{#each ( getCurrent 'cards' ) }}

          {{render 'flip_card.hbs' this}}

        {{/each}}

      </div>


      <!--X INLINE ELEMENTS HERE -->
    </div><!-- X .contentInner -->
  </div><!-- X .content -->
</div><!--X SECTION {{getSection}} -->
